{
  "compilerOptions": {
    "target": "ES2020",
    "module": "esnext",
    "strict": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "isolatedModules": false,
    "lib": ["ESNext", "WebWorker"],
    "noEmit": true
  }
}